                    }
                },
                {
                    label: '优先级', name: 'Priority', index: 'Priority', width: 40, align:'center', sortable: true,
                    formatter: function (cellvalue, options, rowObject) {
                        return cellvalue == 1 ? "<i class=\"fa fa-toggle-on\"></i>" : "<i class=\"fa fa-toggle-off\"></i>";
                    }
                },
                {
                    label: '是否专用', name: 'IsDedicated', index: 'IsDedicated', width: 40, align:'center', sortable: true,
                    formatter: function (cellvalue, options, rowObject) {
                        return cellvalue == 1 ? "<i class=\"fa fa-toggle-on\"></i>" : "<i class=\"fa fa-toggle-off\"></i>";
                    }
