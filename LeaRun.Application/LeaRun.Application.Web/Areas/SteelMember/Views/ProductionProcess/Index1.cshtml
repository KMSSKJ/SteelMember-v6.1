    //加载表格
    var subgrid_table_id;
    var a = 0;
    var OrderId;
    var ProductionedQuantity = 0;
    var SelfDetectNumber = 0;
    var QualityInspectionNumber = 0;

    function GetGrid() {
        var selectedRowIndex = 0;

        var $gridTable = $('#gridTable');
        $gridTable.jqGrid({
            autowidth: true,
            height: $(window).height() - 175,
            url: "../../SteelMember/MemberProductionOrder/GetPageListJsonProcess",/*?queryJson=" + JSON.stringify(queryJson)*/
            datatype: "json",
            colModel: [
                { label: '主键', name: 'OrderId', index: 'OrderId', hidden: true },
                { label: '订单编号', name: 'OrderNumbering', index: 'OrderNumbering', width: 190, align: 'center', sortable: true },
                { label: '所属工程', name: 'Category', index: 'Category', width: 190, align: 'center', sortable: true },
                { label: '制单人', name: 'CreateMan', index: 'CreateMan', width: 100, align: 'center', sortable: true },
                {
                    label: '制单时间', name: 'CreateTime', index: 'CreateTime', width: 90, align: 'center', sortable: true,
                    formatter: function (cellvalue, options, rowObject) {
                        return formatDate(cellvalue, 'yyyy-MM-dd');//'yyyy-MM-dd hh:mm:ss'
                    }
                },
                {
                    label: '预计完成时间', name: 'EstimatedFinishTime', index: 'EstimatedFinishTime', width: 90, align: 'center', sortable: true,
                    formatter: function (cellvalue, options, rowObject) {
                        return formatDate(cellvalue, 'yyyy-MM-dd');//'yyyy-MM-dd hh:mm:ss'
                    }
                },
                //{
                //    label: '优先级', name: 'Priority', index: 'Priority', width: 40, align: 'center', sortable: true,
                //    formatter: function (cellvalue, options, rowObject) {
                //        return cellvalue == 1 ? "<i class=\"fa fa-toggle-on\"></i>" : "<i class=\"fa fa-toggle-off\"></i>";
                //    }
                //},
                //{
                //    label: '是否专用', name: 'IsDedicated', index: 'IsDedicated', width: 40, align: 'center', sortable: true,
                //    formatter: function (cellvalue, options, rowObject) {
                //        return cellvalue == 1 ? "<i class=\"fa fa-toggle-on\"></i>" : "<i class=\"fa fa-toggle-off\"></i>";
                //    }
                //},
                {
                    label: '订单确认状态', name: 'IsConfirm', index: 'IsConfirm', width: 55, align: 'center', sortable: true,
                    formatter: function (cellvalue, options, rowObject) {
                        if (cellvalue == 0) {
                            return "<span class=\"label label-info\">未确认</span>";
                        } else if (cellvalue == 1) {
                            return "<span class=\"label label-success\">已确认</span>";
                        } else if (cellvalue == 2) {
                            return "<span class=\"label label-warning\">已生产</span>";
                        } else if (cellvalue == 3) {
                            return "<span class=\"label label-danger\">已拒绝</span>";
                        }
                    }
                },
                //{
                //    label: '材料领取状态', name: 'IsReceiveRawMaterial', index: 'IsReceiveRawMaterial', width: 55, align:'center', sortable: true,
                //    formatter: function (cellvalue, options, rowObject) {
                //        if (cellvalue == 0) {
                //            return "<span class=\"label label-info\">未领取</span>";
                //        } else if (cellvalue == 1) {
                //            return "<span class=\"label label-success\">已领取</span>";
                //        }
                //    }
                //},
                {
                    label: '生产状态', name: 'ProductionStatus', index: 'ProductionStatus', width: 55, align: 'center', sortable: true,
                    formatter: function (cellvalue, options, rowObject) {
                        if (cellvalue == 0) {
                            return "<span class=\"label label-info\">未生产</span>";
                        } else if (cellvalue == 1) {
                            return "<span class=\"label label-danger\">生产中</span>";
                        } else {
                            return "<span class=\"label label-success\">已完成</span>";
                        }
                    }
