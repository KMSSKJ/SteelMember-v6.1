    //初始化页面
    function InitialPage() {
        //layout布局
        $('#layout').layout({
            applyDemoStyles: true,
            onresize: function () {
                $(window).resize()
            }
        });
        $('.profile-nav').height($(window).height() - 50);
        //resize重设(表格、树形)宽高
        $(window).resize(function (e) {
            window.setTimeout(function () {
                $('#gridTable').setGridWidth(($('.gridPanel').width()));
                $("#gridTable").setGridHeight($(window).height() - 175);
                $('#gridTable_OutLibrary').setGridWidth(($('.gridPanel').width()));
                $("#gridTable_OutLibrary").setGridHeight($(window).height() - 175);
                $('.profile-nav').height($(window).height() - 50);
                //$("#itemTree").setTreeHeight($(window).height() - 52);
            }, 200);
            e.stopPropagation();
        });
    }
    //汇总
    function btn_summary() {
        dialogOpen({
            id: 'MemberSummary',
            title: '生产汇总',
            url: '/SteelMember/ProductionProcess/MemberSummary',
            width: '900px',
            height: '700px',
            callBack: function (iframeId) {
                top.frames[iframeId].btn_add_();
            }
        });
    }

    //待确认
    function btn_ToBeConfirmed() {
        $("#CenterTitle").html("-生产信息-待确认");
        document.getElementById("ProductionInformation").style.display = "block";
        document.getElementById("ProductionBtn").style.display = "block";
        document.getElementById("btn_Search").style.display = "block";
        document.getElementById("OutLibraryInformation").style.display = "none";
        document.getElementById("OutLibraryBtn").style.display = "none";
        document.getElementById("btn_Search_OutLibrary").style.display = "none";
        queryJson = {
            BeginTime: $("#txt_BeginTime").val(),
            EndTime: $("#txt_EndTime").val(),
            IsConfirm: 0,
            IsPassed: 1
        };
        $('#gridTable').jqGrid('setGridParam', {
            postData: { queryJson: JSON.stringify(queryJson) },
            page: 1
        }).trigger('reloadGrid');
    }

