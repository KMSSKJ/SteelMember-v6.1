    //确认订单
    function btn_IsConfirmOrder(type) {
        var keyValues = $('#gridTable').jqGridRowValue('OrderId');
        var IsConfirm = $('#gridTable').jqGridRowValue('IsConfirm');
        if (IsConfirm != "<span class=\"label label-info\">未确认</span>") {
            dialogMsg("该订单已确认，不能重复操作", 0);
            return false;
        }
        if (type == 1) {
            if (checkedArray(keyValues)) {
                $.SaveForm({
                    url: "../../SteelMember/ProductionProcess/ConfirmOrder?keyValues=" + keyValues,
                    param: {},
                    loading: "正在保存数据...",
                    success: function () {
                        $.currentIframe().$("#gridTable").trigger("reloadGrid");
                    }
                })
            }
        } else {
            if (checkedRow(keyValues)) {
                dialogOpen({
                    id: 'NotConfirmOrderForm',
                    title: '拒绝原因',
                    url: '/SteelMember/ProductionProcess/NotConfirmOrder?keyValues=' + keyValues,
                    width: '400px',
                    height: '300px',
                    callBack: function (iframeId) {
                        top.frames[iframeId].AcceptClick();
                    }
                });
            }
        }
    }

    //领取材料
    function btn_receiveRawMaterial() {
        var keyValue = $('#gridTable').jqGridRowValue('OrderId');
        var IsConfirm = $('#gridTable').jqGridRowValue('IsConfirm');
        var IsReceive = $('#gridTable').jqGridRowValue('IsReceiveRawMaterial');

        if (checkedArray(keyValue)) {
            var IsConfirmArr = IsConfirm.split(",");
            if (IsConfirmArr.length > 0) {
                for (var i in IsConfirmArr) {
                    if (IsConfirmArr[i] != "<span class=\"label label-success\">已确认</span>") {
                        dialogMsg("所选数据中存在不属于“已确认”的数据，请重新选择", 0);
                        return false;
                    }
                }
            }

            var IsReceiveArr = IsReceive.split(",");
            if (IsReceiveArr.length > 0) {
                for (var i in IsReceiveArr) {
                    IsReceive = IsReceiveArr[i] == "<span class=\"label label-success\">已领取</span>" ? 1 : 0;
                    if (IsReceive == 1) {
                        dialogMsg("所选数据中存在“已领取”的数据，请重新选择", 0);
                        return false;
                    }
                }
            }

            //dialogConfirm("注：您确定要领取所选中的所有材料吗？", function (r) {
            //    if (r) {
                    dialogOpen({
                        id: 'ReceiveRawMaterialIndex',
                        title: '领取材料',
                        url: '/SteelMember/ProductionProcess/ReceiveRawMaterialIndex?keyValue=' + keyValue + '&IsReceiveRawMaterial=' + IsReceive,
                        width: '1000px',
                        height: '750px',
                        //btn: null,
                        callBack: function (iframeId) {
                            top.frames[iframeId].AcceptClick();
                        }
                    });
            //    }
            //});
        }
    }

    //生产填报
    function btn_productionCompleted() {
        var keyValue = $('#gridTable').jqGridRowValue('OrderId');
        var IsReceiveRawMaterial = $('#gridTable').jqGridRowValue('IsReceiveRawMaterial');
        if (checkedRow(keyValue)) {
            if (IsReceiveRawMaterial == "<span class=\"label label-success\">已领取</span>") {
                dialogOpen({
                    id: 'Form',
                    title: '生产填报',
                    url: '/SteelMember/ProductionProcess/ProductionNumberForm?keyValue=' + keyValue,
                    width: '1000px',
                    height: '750px',
                    callBack: function (iframeId) {
                        top.frames[iframeId].AcceptClick();
                    }
                })
            } else {
                dialogMsg("生产材料未领取", 0)
            }
        }
    }

    //工厂自检填报
    function btn_factoryQualityInspection() {
        var keyValue = $('#gridTable').jqGridRowValue('OrderId');
        var ProductionStatus = $('#gridTable').jqGridRowValue('ProductionStatus');
        var SelfDetectStatus = $('#gridTable').jqGridRowValue('SelfDetectStatus');
        if (SelfDetectStatus == "<span class=\"label label-success\">已完成</span>") {
            dialogMsg("该构件已全部检测", 0);
            return false;
        }

        if (checkedRow(keyValue)) {
            if (ProductionStatus = "<span class=\"label label-success\">已完成</span>") {
                dialogOpen({
                    id: 'Form',
                    title: '工厂自检填报',
                    url: '/SteelMember/ProductionProcess/SelfDetectForm?keyValue=' + keyValue,
                    width: '1000px',
                    height: '750px',
                    callBack: function (iframeId) {
                        top.frames[iframeId].AcceptClick();
                    }
                })
            }
        }
        else {
            dialogMsg('构件生产未完成', 0);
        }
    }

    //监理质检填报
    function btn_supervisionQualityInspection() {
        var keyValue = $('#gridTable').jqGridRowValue('OrderId');
        var SelfDetectStatus = $('#gridTable').jqGridRowValue('SelfDetectStatus');

        var QualityInspectionStatus = $('#gridTable').jqGridRowValue('QualityInspectionStatus');
        if (QualityInspectionStatus == "<span class=\"label label-success\">已完成</span>") {
            dialogMsg("该构件已全部检测", 0);
            return false;
        }
        if (checkedRow(keyValue)) {
            if (SelfDetectStatus = "<span class=\"label label-success\">已完成</span>") {
                dialogOpen({
                    id: 'Form',
                    title: '监理质检填报',
                    url: '/SteelMember/ProductionProcess/QualityInspectionForm?keyValue=' + keyValue,
                    width: '1000px',
                    height: '750px',
                    callBack: function (iframeId) {
                        top.frames[iframeId].AcceptClick();
                    }
                })
            } else {
                dialogMsg('工厂质检未完成', 0);
            }
        }
    }

    //打包
    function btn_package() {
        var keyValue = $("#gridTable").jqGridRowValue("OrderId");
        var QualityInspectionStatus = $('#QualityInspectionStatus').jqGridRowValue('QualityInspectionStatus');
        if (checkedRow(keyValue)) {
            if (QualityInspectionStatus == "<span class=\"label label-success\">已完成</span>") {
                dialogMsg("监理质检未完成", 0);
                return false;
            }
            $.SaveForm({
                url: "../../SteelMember/ProductionProcess/Package?keyValue=" + keyValue,
                param: {},
                loading: "正在处理...",
