                    label: '备注', name: 'Description', index: 'Description', width: 200, align: 'center',
                    formatter: function (cellvalue, options, rowObject) {
                        if (rowObject.Description == null || rowObject.Description == "undefined") {
                            return "";
                        } else {
                            return "<p style='color: red'>" + cellvalue + "</p>";
                        }
                    }
                },
            ],
            viewrecords: true,
            rowNum: 30,
            rowList: [30, 50, 100],
            pager: "#gridPager",
            sortname: 'OrderId',
            sortorder: 'desc',
            rownumbers: true,
            shrinkToFit: false,
            gridview: true,
            multiselect: true,
            multiboxonly: true,
            onSelectRow: function (row_id) {
                selectedRowIndex = $('#' + this.id).getGridParam('selrow');
                //单击获取从表中的数据
                OrderId = $gridTable.jqGrid('getRowData', row_id)['OrderId'];
                $.ajax({
                    url: "../../SteelMember/MemberProductionOrder/GetDetailsJson",
                    data: { keyValue: OrderId },
                    type: "get",
                    dataType: "json",
                    success: function (data) {
                        for (var i = 0; i < data.length; i++) {
                            ProductionedQuantity += data[i].ProductionedQuantity;
                            SelfDetectNumber += data[i].SelfDetectNumber;
                            QualityInspectionNumber += data[i].QualityInspectionNumber;
                        }
                    }
                });
            },
            gridComplete: function () {
                $('#' + this.id).setSelection(selectedRowIndex, false);
            },
            subGrid: true,
            subGridRowExpanded: function (subgrid_id, row_id) {
                if (a != 0) {
                    $("#gridTable").collapseSubGridRow(a);
                }

                OrderId = $gridTable.jqGrid('getRowData', row_id)['OrderId'];
                subgrid_table_id = subgrid_id + "_t";
                $("#" + subgrid_id).html("<table id='" + subgrid_table_id + "'></table>");
                $("#" + subgrid_table_id).jqGrid({
                    url: "../../SteelMember/MemberProductionOrder/GetDetailsJson",
                    postData: { keyValue: OrderId },
                    datatype: "json",
                    height: "100%",
                    colModel: [
                        { label: 'InfoId', name: 'InfoId', index: 'InfoId', hidden: true },
                        { label: 'OrderId', name: 'OrderId', index: 'OrderId', hidden: true },
                        { label: 'MemberId', name: 'MemberId', index: 'MemberId', hidden: true },
                        { label: '构件编号', name: "MemberNumbering", align: 'center', width: 180, sortable: false },
                        { label: '构件类型', name: "Category", align: 'center', width: 140, sortable: false },
                        { label: '构件名称', name: "MemberName", align: 'center', width: 120, sortable: false },
                        { label: '单位', name: "UnitId", align: ' center', width: 50, sortable: false, align: 'center' },
                        { label: '申请量', name: 'MemberNumber', align: 'center', width: 60, sortable: false },
                        { label: '本次生产量', name: 'ProductionedQuantity', align: 'center', width: 60, sortable: false },
                        { label: '工厂质检合格量', name: 'SelfDetectNumber', align: 'center', width: 80, sortable: false },
                        { label: '工厂质检情况', name: 'SelfDetectRemarks', align: 'center', width: 80, sortable: false },
                        { label: '监理质检合格量', name: 'QualityInspectionNumber', align: 'center', width: 80, sortable: false },
                        { label: '监理质检情况', name: 'QualityInspectionRemarks', align: 'center', width: 80, sortable: false },
                        { label: '已入库量', name: 'WarehousedQuantity', align: 'center', width: 60, sortable: false },
                        { label: '废品量', name: 'WasteQuantity', align: 'center', width: 60, sortable: false },
                        {
                            label: '构件详情', name: '', index: '', width: 80, align: 'center', sortable: false,
                            formatter: function (cellvalue, options, rowObject) {
                                return "<a href='#'onclick='btn_drawing()' ><span class='label label-success'>查看</span></a >";
                            }
                        }
                    ],
                    caption: "构件信息",
                    rowNum: "1000",
                    rownumbers: true,
                    shrinkToFit: false,
                    gridview: true,
                    hidegrid: false
                });
                a = row_id;
            }
        });
        //查询事件
        $("#btn_Search").click(function () {
            var queryJson = {
                //condition: $("#queryCondition").find('.dropdown-text').attr('data-value'),
                keyword: $("#txt_Keyword").val(),
            };
            $gridTable.jqGrid('setGridParam', {
                postData: { queryJson: JSON.stringify(queryJson) },
                page: 1
            }).trigger('reloadGrid');
        });
        //查询回车
        $('#txt_Keyword').bind('keypress', function (event) {
            if (event.keyCode == "13") {
                $('#btn_Search').trigger("click");
            }
        });
    }

