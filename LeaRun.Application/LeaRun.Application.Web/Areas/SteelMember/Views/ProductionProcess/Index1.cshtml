                { label: '订单编号', name: 'OrderNumbering', index: 'OrderNumbering', width: 190, align: 'center', sortable: true },
                { label: '所属工程', name: 'Category', index: 'Category', width: 190, align:'center', sortable: true },
                { label: '制单人', name: 'CreateMan', index: 'CreateMan', width: 100, align:'center', sortable: true },
                {
                    label: '制单时间', name: 'CreateTime', index: 'CreateTime', width: 90, align:'center', sortable: true,
                    formatter: function (cellvalue, options, rowObject) {
                        return formatDate(cellvalue, 'yyyy-MM-dd');//'yyyy-MM-dd hh:mm:ss'
                    }
                },
                {
                    label: '预计完成时间', name: 'EstimatedFinishTime', index: 'EstimatedFinishTime', width: 90, align:'center', sortable: true,
                    formatter: function (cellvalue, options, rowObject) {
