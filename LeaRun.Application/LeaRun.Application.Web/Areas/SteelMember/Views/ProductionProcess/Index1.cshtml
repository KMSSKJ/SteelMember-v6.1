                {
                    label: '订单确认状态', name: 'IsConfirm', index: 'IsConfirm', width: 55, align:'center', sortable: true,
                    formatter: function (cellvalue, options, rowObject) {
                        if (cellvalue == 0) {
                            return "<span class=\"label label-info\">未确认</span>";
                        } else if (cellvalue == 1) {
                            return "<span class=\"label label-success\">已确认</span>";
                        } else if (cellvalue == 2) {
                            return "<span class=\"label label-warning\">已生产</span>";
                        } else if (cellvalue == 3) {
                            return "<span class=\"label label-danger\">已拒绝</span>";
