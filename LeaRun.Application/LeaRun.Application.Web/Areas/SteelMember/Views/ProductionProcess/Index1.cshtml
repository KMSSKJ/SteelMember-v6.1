                        }
                    }
                }, {
                    label: '打包', name: 'IsPackage', align:  'center', width: 55, sortable: false,
                    formatter: function (cellvalue, options, rowObject) {
                        if (cellvalue == 0) {
                            return "<span class=\"label label-info\">未进行</span>";
                        } else if (cellvalue == 1) {
