                    label: '监理质检', name: 'QualityInspectionStatus', index: 'QualityInspectionStatus', width: 55, align: 'center', sortable: true,
                    formatter: function (cellvalue, options, rowObject) {
                        if (cellvalue == 0) {
                            return "<span class=\"label label-info\">未进行</span>";
                        } else if (cellvalue == 1) {
                            return "<span class=\"label label-danger\">质检中</span>";
                        } else {
