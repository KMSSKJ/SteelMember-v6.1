    //确认订单
    function btn_IsConfirmOrder(type) {
        var keyValues = $('#gridTable').jqGridRowValue('OrderId');
        var IsConfirm = $('#gridTable').jqGridRowValue('IsConfirm');
        if (IsConfirm != "<span class=\"label label-info\">未确认</span>") {
            dialogMsg("该订单已确认，不能重复操作", 0);
            return false;
        }
        if (type == 1) {
            if (checkedArray(keyValues)) {
                $.SaveForm({
                    url: "../../SteelMember/ProductionProcess/ConfirmOrder?keyValues=" + keyValues,
                    param: {},
                    loading: "正在保存数据...",
                    success: function () {
                        $.currentIframe().$("#gridTable").trigger("reloadGrid");
                    }
                })
            }
        } else {
            if (checkedRow(keyValues)) {
                dialogOpen({
                    id: 'NotConfirmOrderForm',
                    title: '拒绝原因',
                    url: '/SteelMember/ProductionProcess/NotConfirmOrder?keyValues=' + keyValues,
                    width: '400px',
                    height: '300px',
                    callBack: function (iframeId) {
                        top.frames[iframeId].AcceptClick();
                    }
                });
            }
        }
    }

    //领取材料
    function btn_receiveRawMaterial() {
        var keyValue = $('#gridTable').jqGridRowValue('OrderId');
        var IsConfirm = $('#gridTable').jqGridRowValue('IsConfirm');
        var IsReceive = $('#gridTable').jqGridRowValue('IsReceiveRawMaterial');

        if (checkedArray(keyValue)) {
            var IsConfirmArr = IsConfirm.split(",");
            if (IsConfirmArr.length > 0) {
                for (var i in IsConfirmArr) {
                    if (IsConfirmArr[i] != "<span class=\"label label-success\">已确认</span>") {
                        dialogMsg("所选数据中存在不属于“已确认”的数据，请重新选择", 0);
                        return false;
                    }
                }
            }

            var IsReceiveArr = IsReceive.split(",");
            if (IsReceiveArr.length > 0) {
                for (var i in IsReceiveArr) {
                    IsReceive = IsReceiveArr[i] == "<span class=\"label label-success\">已领取</span>" ? 1 : 0;
                    if (IsReceive == 1) {
                        dialogMsg("所选数据中存在“已领取”的数据，请重新选择", 0);
                        return false;
                    }
                }
            }

            //dialogConfirm("注：您确定要领取所选中的所有材料吗？", function (r) {
            //    if (r) {
            dialogOpen({
                id: 'ReceiveRawMaterialIndex',
                title: '领取材料',
                url: '/SteelMember/ProductionProcess/ReceiveRawMaterialIndex?keyValue=' + keyValue + '&IsReceiveRawMaterial=' + IsReceive,
                width: '1000px',
                height: '750px',
                //btn: null,
                callBack: function (iframeId) {
                    top.frames[iframeId].AcceptClick();
                }
            });
            //    }
            //});
        }
    }

    //生产填报
    function btn_productionCompleted() {
        var keyValue = $('#gridTable').jqGridRowValue('OrderId');
        //var IsReceiveRawMaterial = $('#gridTable').jqGridRowValue('IsReceiveRawMaterial');
        var IsConfirm = $('#gridTable').jqGridRowValue('IsConfirm');
        if (checkedRow(keyValue)) {
            if (IsConfirm == "<span class=\"label label-success\">已确认</span>") {
                if (Number(ProductionedQuantity) != 0) {
                    dialogMsg("该单下本次生产量未入库！", 0)
                    return false;
                }
                dialogOpen({
                    id: 'Form',
                    title: '生产填报',
                    url: '/SteelMember/ProductionProcess/ProductionNumberForm?keyValue=' + keyValue,
                    width: '1000px',
                    height: '750px',
                    callBack: function (iframeId) {
                        top.frames[iframeId].AcceptClick();
                    }
                })
            } else {
                dialogMsg("该数据未确认！", 0)
            }
        }
    }

    //工厂自检填报
    function btn_factoryQualityInspection() {
        var keyValue = $('#gridTable').jqGridRowValue('OrderId');
        var ProductionStatus = $('#gridTable').jqGridRowValue('ProductionStatus');
        var SelfDetectStatus = $('#gridTable').jqGridRowValue('SelfDetectStatus');
        if (SelfDetectStatus == "<span class=\"label label-success\">已完成</span>") {
            dialogMsg("该构件已全部检测", 0);
            return false;
        }
        if (checkedRow(keyValue)) {
            if (ProductionStatus = "<span class=\"label label-success\">已完成</span>") {
                if (Number(SelfDetectNumber) != 0) {
                    dialogMsg("该单下本次自检量未入库！", 0);
                    return false;
                }

                dialogOpen({
                    id: 'Form',
                    title: '工厂自检填报',
                    url: '/SteelMember/ProductionProcess/SelfDetectForm?keyValue=' + keyValue,
                    width: '1000px',
                    height: '750px',
                    callBack: function (iframeId) {
                        top.frames[iframeId].AcceptClick();
                    }
                })
            }
        }
        else {
            dialogMsg('构件生产未完成', 0);
        }
    }

    //监理质检填报
    function btn_supervisionQualityInspection() {
        var keyValue = $('#gridTable').jqGridRowValue('OrderId');
        var SelfDetectStatus = $('#gridTable').jqGridRowValue('SelfDetectStatus');

        var QualityInspectionStatus = $('#gridTable').jqGridRowValue('QualityInspectionStatus');
        if (QualityInspectionStatus == "<span class=\"label label-success\">已完成</span>") {
            dialogMsg("该构件已全部检测", 0);
            return false;
        }
        if (checkedRow(keyValue)) {
            if (SelfDetectStatus = "<span class=\"label label-success\">已完成</span>") {

                if (Number(QualityInspectionNumber) != 0) {
                    dialogMsg("该单下本次质检量未入库！", 0);
                    return false;
                }

                dialogOpen({
                    id: 'Form',
                    title: '监理质检填报',
                    url: '/SteelMember/ProductionProcess/QualityInspectionForm?keyValue=' + keyValue,
                    width: '1000px',
                    height: '750px',
                    callBack: function (iframeId) {
                        top.frames[iframeId].AcceptClick();
                    }
                })
            } else {
                dialogMsg('工厂质检未完成', 0);
            }
        }
    }

    //打包
    function btn_package() {
        var keyValue = $("#gridTable").jqGridRowValue("OrderId");
        var QualityInspectionStatus = $('#QualityInspectionStatus').jqGridRowValue('QualityInspectionStatus');
        if (checkedRow(keyValue)) {
            if (QualityInspectionStatus == "<span class=\"label label-success\">已完成</span>") {
                dialogMsg("监理质检未完成", 0);
                return false;
            }
            $.SaveForm({
                url: "../../SteelMember/ProductionProcess/Package?keyValue=" + keyValue,
                param: {},
                loading: "正在处理...",
                success: function () {
                    $.currentIframe().$("#gridTable").trigger("reloadGrid");
                }
            })
        }
    }

    //二维码信息及打印
    function btn_print() {
        if (subgrid_table_id != undefined) {
            var IsPackage = $("#gridTable").jqGridRowValue("IsPackage");
            var keyValue = $("#" + subgrid_table_id).jqGridRowValue("MemberId"); //
            var QualityInspectionNumber = $("#" + subgrid_table_id).jqGridRowValue("QualityInspectionNumber");
            var MemberNumber = $("#" + subgrid_table_id).jqGridRowValue("MemberNumber");
            if (checkedRow(keyValue)) {
                if (QualityInspectionNumber == MemberNumber && IsPackage == "<span class=\"label label-success\">已完成</span>") {
                    dialogOpen({
                        id: 'QRCodeForm',
                        title: '打印信息',
                        url: '/SteelMember/ProductionProcess/QRCodeForm?keyValue=' + keyValue + "&OrderId=" + OrderId,
                        width: '300px',
                        height: '400px',
                        callBack: function (iframeId) {
                            top.frames[iframeId].AcceptClick();
                        }
                    })
                } else {
                    dialogMsg("构件生产或打包未完成", 0);
                }
            } else {
                dialogMsg("请选择构件", 0);
            }
        } else {
            dialogMsg("请选择订单下的构件", 0);
        }
    };

    //详细
    function btn_details() {
        var keyValue = $('#gridTable').jqGridRowValue('OrderId');
        if (checkedRow(keyValue)) {
            dialogOpen({
                id: 'Form',
                title: '详细构件生产订单',
                url: '/SteelMember/ProductionProcess/Form?keyValue=' + keyValue + '&isDeltail=true',
                width: '1000px',
                height: '750px',
                btn: null,
                callBack: function (iframeId) {
                    top.frames[iframeId].AcceptClick();
                }
            })
        }
    }

    // 提交
    function btn_submitReview() {
        var keyValues = $('#gridTable').jqGridRowValue('OrderId');
        var reviews = $('#gridTable').jqGridRowValue('IsSubmit');
        if (checkedArray(keyValues)) {
            var reviewsArr = reviews.split(",");
            if (reviewsArr.length > 0) {
                for (var i in reviewsArr) {
                    if (reviewsArr[i] == "<i class=\"fa fa-toggle-on\"></i>") {
                        dialogMsg("所选数据中包含已提交的数据，请重新选择！", 0);
                        return false;
                        break;
                    }
                }
            }
            dialogConfirm("注：您确定要提交所选中的数据吗？", function (r) {
                if (r) {
                    Loading(true, "正在提交...");
                    window.setTimeout(function () {
                        $.ajax({
                            url: "../../SteelMember/ProductionProcess/SubmitReview",
                            data: { keyValues: keyValues },
                            type: "post",
                            dataType: "json",
                            success: function (data) {
                                if (data.type == "3") {
                                    dialogAlert(data.message, -1);
                                } else {
                                    Loading(false);
                                    dialogMsg(data.message, 1);
                                }
                                $('#gridTable').trigger('reloadGrid');
                            }
                        });
                    }, 500);
                }
            });
        }
    }

    //审核
    function btn_passed(type) {
        var keyValues = $('#gridTable').jqGridRowValue('OrderId');
        var reviews = $('#gridTable').jqGridRowValue('IsSubmit');
        var passeds = $('#gridTable').jqGridRowValue('IsPassed');
        if (checkedArray(keyValues)) {
            var reviewsArr = reviews.split(",");
            var passedsArr = passeds.split(",");
            if (reviewsArr.length > 0) {
                for (var i in reviewsArr) {
                    if (reviewsArr[i] == "<i class=\"fa fa-toggle-off\"></i>") {
                        dialogMsg("所选数据中包含未提交的数据，请重新选择！", 0);
                        return false;
                        break;
                    }
                }
            }
            if (passedsArr.length > 0) {
                for (var i in passedsArr) {
                    if (passedsArr[i] == "<span class=\"label label-success\">通过</span>" || passedsArr[i] == "<span class=\"label label-danger\">失败</span>") {
                        dialogMsg("所选数据中包含已审核的数据，请重新选择！", 0);
                        return false;
                        break;
                    }
                }
            }
            var message = "";
            if (type == 1) {
                message = "注：您确定要对所选中的数据进行审核通过吗？";
            } else {
                message = "注：您确定要对所选中的数据进行审核失败吗？";
            }
            dialogConfirm(message, function (r) {
                if (r) {
                    Loading(true, "正在处理...");
                    window.setTimeout(function () {
                        $.ajax({
                            url: "../../SteelMember/ProductionProcess/ReviewOperation",
                            data: { keyValues: keyValues, type: type },
                            type: "post",
                            dataType: "json",
                            success: function (data) {
                                if (data.type == "3") {
                                    dialogAlert(data.message, -1);
                                } else {
                                    Loading(false);
                                    dialogMsg(data.message, 1);
                                }
                                $('#gridTable').trigger('reloadGrid');
                            }
                        });
                    }, 500);
                }
            });
        }
    }

    //构件图纸/模型管理
    function btn_drawing() {
        var KeyValue = $("#" + subgrid_table_id).jqGridRowValue("MemberId");
        if (checkedRow(KeyValue)) {
            dialogOpen({
                id: "DrawingManagement",
                title: '构件信息',
                url: '../../SteelMember/MemberLibrary/DrawingManagement?KeyValue=' + KeyValue,
                width: "705px",
                height: "630px",
                btn: null
            });
        }
    }

    // 收货
    function btn_Received() {
        var keyValues = $('#gridTable_OutLibrary').jqGridRowValue('CollarId');
        var IsReceived = $('#gridTable_OutLibrary').jqGridRowValue('IsReceived');
        if (checkedArray(keyValues)) {
            var reviewsArr = IsReceived.split(",");
            if (reviewsArr.length > 0) {
                for (var i in reviewsArr) {
                    if (reviewsArr[i] == "<i class=\"fa fa-toggle-on\"></i>") {
                        dialogMsg("所选数据中包含已收货的数据，请重新选择！", 0);
                        return false;
                    }
                }
            }
            Loading(true, "正在提交...");
            window.setTimeout(function () {
                $.ajax({
                    url: "../../SteelMember/MemberCollar/SubmitIsReceived",
                    data: { keyValues: keyValues },
                    type: "post",
                    dataType: "json",
                    success: function (data) {
                        if (data.type == "3") {
                            dialogAlert(data.message, -1);
                        } else {
                            Loading(false);
                            dialogMsg(data.message, 1);
                        }
                        $('#gridTable_OutLibrary').trigger('reloadGrid');
                    }
                });
            }, 500);
        }
    }

    function Search() {
        dialogOpen({
            id: "AdvancedSearchMemberForm",
            title: '高级查询',
            url: '/Utility/AdvancedSearchMemberForm?SubProjectId=' + _Category,
            width: "670px",
            height: "180px",
            callBack: function (iframeId) {
                top.frames[iframeId].AcceptClick();
            },
            btn: null,
        });
    }
</script>

