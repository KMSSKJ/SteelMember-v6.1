                        { label: '构件名称', name: "MemberName", align:  'center', width: 120, sortable: false },
                        { label: '单位', name: "UnitId", align:  ' center', width: 50, sortable: false, align: 'center' },
                        { label: '计划生产量', name: 'MemberNumber', align:  'center', width: 60, sortable: false },
                        { label: '已合格量', name: 'QualifiedQuantity', align:  'center', width: 60, sortable: false },
                        { label: '已生产量', name: 'ProductionedQuantity', align:  'center', width: 60, sortable: false },
                        //{
                        //    label: '本次生产量', name: 'ThisTimeProductionQuantity', align: 'center', width: 60, sortable: false,
                        //    formatter: function (cellvalue, options, rowObject) {
                        //        return 0;
                        //    } },
                        { label: '工厂质检合格量', name: 'SelfDetectNumber', align:  'center', width: 80, sortable: false },
                        { label: '工厂质检情况', name: 'SelfDetectRemarks', align:  'center', width: 80, sortable: false },
                        { label: '监理质检合格量', name: 'QualityInspectionNumber', align:  'center', width: 80, sortable: false },
                        { label: '监理质检情况', name: 'QualityInspectionRemarks', align: 'center', width: 80, sortable: false },
                        { label: '已入库量', name: 'WarehousedQuantity', align: 'center', width: 60, sortable: false },
                        { label: '废品量', name: 'WasteQuantity', align: 'center', width: 60, sortable: false },
                        {
                            label: '构件详情', name: '', index: '', width: 80, align: 'center', sortable: false,
                            formatter: function (cellvalue, options, rowObject) {
                                return "<a href='#'onclick='btn_drawing()' ><span class='label label-success'>查看</span></a >";
                            }
                        }
                    ],
                    caption: "明细",
                    rowNum: "1000",
                    rownumbers: true,
                    shrinkToFit: false,
                    gridview: true,
                    hidegrid: false
                });
                a = row_id;
            }
        });

        //var queryJson = {
