@{
    ViewBag.Title = "查询过滤";
        Layout = "~/Views/Shared/_LayoutIndex.cshtml";
}

<script>
    //获取条件表格JSON
    function GetConditionJson() {
        var JsonData = [];
        $("#grid_condition tbody tr").each(function () {
            var MemberModel = $(this).find('td:eq(0)').find('input').val();
            var InBeginTime = $(this).find('td:eq(1)').find('input').val();
            var InEndTime = $(this).find('td:eq(2)').find('input').val();
            var Class = $(this).find('td:eq(3)').find('select').val();
            var tdData = {
                    MemberModel: MemberModel,
                    InBeginTime: InBeginTime,
                    InEndTime: InEndTime,
                    Class: Class
                }
                JsonData.push(tdData);
        });
        return JsonData;
    }
    //回调查询确认事件
    function QueryClick(callBack) {
        callBack(JSON.stringify(GetConditionJson()));
        closeDialog();
    }

</script>
<br />
<br />
<br />
<table id="grid_condition" border="0" class="form-find" style="height: 30px;">
    <tr>
        <th>构件型号：</th>
        <td>
            <input id="MemberModel" class="txt" style="width:120px;" />
        </td>
        <th>入系统时间：</th>
        <td>
            <input id="InBeginTime" type="text" class="txt Wdate" onfocus="WdatePicker({maxDate:'%y-%M-%d'})" style="width:90px;" />
            至
        </td>
        <td><input id="InEndTime" type="text" class="txt Wdate" onfocus="WdatePicker({maxDate:'%y-%M-%d'})" style="width:90px;" /></td>
        <th>类型：</th>
        <td>
            <select id="Class">
                <option value="0">请选择</option>
                <option value="出货">出货</option>
                <option value="入库">入库</option>
                <option value="退货">退货</option>
            </select>
        </td>
    </tr>
</table>




