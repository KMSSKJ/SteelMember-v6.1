@{;
    ViewBag.Title = "列表页面";
    Layout = "~/Views/Shared/_LayoutIndex.cshtml";
}
<script>
    ;
    $(function () {
        InitialPage();
        GetTree();
        GetGrid();
    });
    //初始化页面
    function InitialPage() {
        //layout布局
        $('#layout').layout({
            applyDemoStyles: true,
            onresize: function () {
                $(window).resize()
            }
        });
        //resize重设(表格、树形)宽高
        $(window).resize(function (e) {
            window.setTimeout(function () {
                $('#gridTable').setGridWidth(($('.gridPanel').width()));
                $("#gridTable").setGridHeight($(window).height() - 170);
                $("#itemTree").setTreeHeight($(window).height() - 52);
            }, 200);
            e.stopPropagation();
        });
    }

    //加载树
    var _Category;
    var HavesChildren;
    var SubProjectName;
    function GetTree() {
        var item = {
            height: $(window).height() - 52,
            param: { Levels: 3 },
            url: '../../SteelMember/SubProject/GetTreeJson',
            onnodeclick: function (item) {
                _Category = item.value;
                HavesChildren = item.hasChildren;
                SubProjectName = item.text;
                $("#CenterTitle").html("-" + SubProjectName);
                $('#btn_Search').trigger("click");
            }
        };
        //初始化
        $("#itemTree").treeview(item);
    }

    //加载表格
    function GetGrid() {
        var selectedRowIndex = 0;
        var $gridTable = $('#gridTable');
        $gridTable.jqGrid({
            autowidth: true,
            height: $(window).height() - 170,
            url: "../../SteelMember/RawMaterialOrder/GetPageListJson",
            datatype: "json",
            colModel: [
                { label: '主键', name: 'OrderId', index: 'OrderId', hidden: true },
                { label: '单号', name: 'OrderNumbering', index: 'OrderNumbering', width: 190, align: 'center', sortable: true },
                { label: '申请人', name: 'CreateMan', index: 'CreateMan', width: 100, align: 'center', sortable: true },
                {
                    label: '制单时间', name: 'CreateTime', index: 'CreateTime', width: 90, align: 'center', sortable: true,
                    formatter: function (cellvalue, options, rowObject) {
                        return formatDate(cellvalue, 'yyyy-MM-dd');//'yyyy-MM-dd hh:mm:ss'
                    }
                },
                {
                    label: '提交状态', name: 'IsSubmit', index: 'IsSubmit', width: 80, align: 'center', sortable: true,
                    formatter: function (cellvalue, options, rowObject) {
                        return cellvalue == 1 ? "<i class=\"fa fa-toggle-on\"></i>" : "<i class=\"fa fa-toggle-off\"></i>";
                    }
                },
                {
                    label: '构件厂长<br/>审核', name: 'ReviewMan1', index: 'ReviewMan1', width: 80, align: 'center', sortable: true, formatter: function (cellvalue, options, rowObject) {
                        if (cellvalue == 0) {
                            return "<span class=\"label label-info\">未审核</span>";
                        } else if (cellvalue == 2) {
                            return "<span class=\"label label-danger\">失败</span>";
                        } else if (cellvalue != null && cellvalue != "") {
                            return "<span class=\"label label-success\">通过</span>";
                        } else {
                            return "";
                        }
                    }
                },
                {
                    label: '工程部长<br/>审核', name: 'ReviewMan2', index: 'ReviewMan2', width: 80, align: 'center', sortable: true, formatter: function (cellvalue, options, rowObject) {
                        if (cellvalue == 0) {
                            return "<span class=\"label label-info\">未审核</span>";
                        } else if (cellvalue == 2) {
                            return "<span class=\"label label-danger\">失败</span>";
                        } else if (cellvalue != null && cellvalue != "") {
                            return "<span class=\"label label-success\">通过</span>";
                        } else {
                            return "";
                        }
                    }
                },
                {
                    label: '物质部长<br/>审核', name: 'ReviewMan3', index: 'ReviewMan3', width: 80, align: 'center', sortable: true, formatter: function (cellvalue, options, rowObject) {
                        if (cellvalue == 0) {
                            return "<span class=\"label label-info\">未审核</span>";
                        } else if (cellvalue == 2) {
                            return "<span class=\"label label-danger\">失败</span>";
                        } else if (cellvalue != null && cellvalue != "") {
                            return "<span class=\"label label-success\">通过</span>";
                        } else {
                            return "";
                        }
                    }
                },
                {
                    label: '副总工审核', name: 'ReviewMan4', index: 'ReviewMan4', width: 80, align: 'center', sortable: true, formatter: function (cellvalue, options, rowObject) {
                        if (cellvalue == 0) {
                            return "<span class=\"label label-info\">未审核</span>";
                        } else if (cellvalue == 2) {
                            return "<span class=\"label label-danger\">失败</span>";
                        } else if (cellvalue != null && cellvalue != "") {
                            return "<span class=\"label label-success\">通过</span>";
                        } else {
                            return "";
                        }
                    }
                },
                {
                    label: '总工审核', name: 'ReviewMan5', index: 'ReviewMan5', width: 80, align: 'center', sortable: true, formatter: function (cellvalue, options, rowObject) {
                        if (cellvalue == 0) {
                            return "<span class=\"label label-info\">未审核</span>";
                        } else if (cellvalue == 2) {
                            return "<span class=\"label label-danger\">失败</span>";
                        } else if (cellvalue != null && cellvalue != "") {
                            return "<span class=\"label label-success\">通过</span>";
                        } else {
                            return "";
                        }
                    }
                },
                {
                    label: '收货状态', name: 'IsReceived', index: 'IsReceived', width: 80, align: 'center', sortable: true,
                    formatter: function (cellvalue, options, rowObject) {
                        return cellvalue == 1 ? "<i class=\"fa fa-toggle-on\"></i>" : "<i class=\"fa fa-toggle-off\"></i>";
                    }
                },
                {
                    label: '备注', name: 'Description', index: 'Description', width: 200, align: 'center', sortable: false,
                    formatter: function (cellvalue, options, rowObject) {
                        if (rowObject.Description == null || rowObject.Description == "undefined") {
                            return "";
                        } else {
                            return "<p style='color: red'>" + cellvalue + "</p>";
                        }
                    }
                },
            ],
            viewrecords: true,
            rowNum: 30,
            rowList: [30, 50, 100],
            pager: "#gridPager",
            sortname: 'CreateTime',
            sortorder: 'desc',
            rownumbers: true,
            shrinkToFit: false,
            gridview: true,
            multiselect: true,
            multiboxonly: true,
            onSelectRow: function () {
                selectedRowIndex = $('#' + this.id).getGridParam('selrow');
            },
            gridComplete: function () {
                $('#' + this.id).setSelection(selectedRowIndex, false);
            },
            subGrid: true,
            subGridRowExpanded: function (subgrid_id, row_id) {
                var keyValue = $gridTable.jqGrid('getRowData', row_id)['OrderId'];
                var subgrid_table_id = subgrid_id + "_t";
                $("#" + subgrid_id).html("<table id='" + subgrid_table_id + "'></table>");
                $("#" + subgrid_table_id).jqGrid({
                    url: "../../SteelMember/RawMaterialOrderInfo/GetListJson",
                    postData: { keyValue: keyValue },
                    datatype: "json",
                    height: "100%",
                    colModel: [
                        { label: 'InfoId', name: 'InfoId', align: 'center', hidden: true },
                        { label: 'RawMaterialId', name: 'RawMaterialId', align: 'center', hidden: true },
                        { label: 'OrderId', name: 'OrderId', index: 'OrderId', hidden: true },
                        { label: '材料类型', name: 'Category', align: 'center', width: 120, sortable: false },
                        { label: '材料名称', name: 'RawMaterialName', align: 'center', width: 180, sortable: false },
                        { label: '牌号/规格/规格', name: 'RawMaterialModel', align: 'center', width: 160, sortable: false },
                        { label: '单位', name: 'UnitId', align: 'center', width: 90, sortable: false },
                        { label: '申请数量', name: 'RawMaterialNumber', align: 'center', width: 100, sortable: false },
                        //{ label: '单价', name: 'Price', align: ' center', width: 80, sortable: false },
                        //{ label: '金额', name: 'PriceAmount', align: ' center', width: 120, sortable: false },
                    ],
                    caption: "材料信息",
                    rowNum: "1000",
                    rownumbers: true,
                    shrinkToFit: false,
                    gridview: true,
                    hidegrid: false
                });
            }
        });

        //查询条件
        $("#queryCondition .dropdown-menu li").click(function () {
            var text = $(this).find('a').html();
            var value = $(this).find('a').attr('data-value');
            $("#queryCondition .dropdown-text").html(text).attr('data-value', value)
        });
        //查询事件
        $("#btn_Search").click(function () {
            var queryJson = {
                // condition: $("#queryCondition").find('.dropdown-text').attr('data-value'),
                condition: "OrderNumbering",
                keyword: $("#txt_Keyword").val(),
                SubProjectId: _Category,
                BeginTime: $("#txt_BeginTime").val(),
                EndTime: $("#txt_EndTime").val()
            }
            $gridTable.jqGrid('setGridParam', {
                postData: { queryJson: JSON.stringify(queryJson) },
                page: 1
            }).trigger('reloadGrid');
        });
        //查询回车
        $('#txt_Keyword').bind('keypress', function (event) {
            if (event.keyCode == "13") {
                $('#btn_Search').trigger("click");
            }
        });
    }

    //新增
    function btn_add() {
        if (!HavesChildren) {
            dialogOpen({
                id: 'Form',
                title: '新增申请单',
                url: '/SteelMember/RawMaterialOrder/Form?category=' + _Category,
                width: '1035px',
                height: '780px',
                callBack: function (iframeId) {
                    top.frames[iframeId].AcceptClick();
                }
            });
        } else {
            dialogMsg("请选择子级工程", 0);
        }
    }
    //编辑
    function btn_edit() {
        var keyValue = $('#gridTable').jqGridRowValue('OrderId');
        var IsReview = $('#gridTable').jqGridRowValue('IsPassed');
        var IsSubmit = $('#gridTable').jqGridRowValue('IsSubmit');
        if (checkedRow(keyValue)) {
            if (IsSubmit != "<i class=\"fa fa-toggle-on\"></i>" || IsReview == "<span class=\"label label-danger\">失败</span>") {
                dialogOpen({
                    id: 'Form',
                    title: '编辑申请单',
                    url: '/SteelMember/RawMaterialOrder/Form?keyValue=' + keyValue,
                    width: '1035px',
                    height: '780px',
                    callBack: function (iframeId) {
                        top.frames[iframeId].AcceptClick();
                    }
                })
            } else {
                dialogMsg("已提交的数据只有审核失败后才能作编辑！", 0);
            }
        }
    }
    //删除
    function btn_delete() {
        var keyValue = $('#gridTable').jqGridRowValue('OrderId');
        var IsReview = $('#gridTable').jqGridRowValue('IsPassed');
        var IsSubmit = $('#gridTable').jqGridRowValue('IsSubmit');
        if (checkedArray(keyValue)) {
            if (IsSubmit != "<i class=\"fa fa-toggle-on\"></i>" || IsReview == "<span class=\"label label-danger\">失败</span>") {
                $.RemoveForm({
                    url: '../../SteelMember/RawMaterialOrder/RemoveForm',
                    param: { keyValue: keyValue },
                    success: function (data) {
                        $('#gridTable').trigger('reloadGrid');
                    }
                })
            } else {
                dialogMsg("已提交的数据只有审核失败后才能作编辑！", 0);
            }
        }
    }
    //详细
    function btn_details() {
        var keyValue = $('#gridTable').jqGridRowValue('OrderId');
        if (checkedRow(keyValue)) {
            dialogOpen({
                id: 'FormDetail',
                title: '材料申请单',
                url: '/SteelMember/RawMaterialOrder/FormDetail?keyValue=' + keyValue,
                width: '1125px',
                height: '760px',
                btn: null,
                callBack: function (iframeId) {
                    top.frames[iframeId].AcceptClick();
                }
            })
        }
    }
    // 提交
    function btn_submitReview() {
        var keyValues = $('#gridTable').jqGridRowValue('OrderId');
        var reviews = $('#gridTable').jqGridRowValue('IsSubmit');
        if (checkedArray(keyValues)) {
            var reviewsArr = reviews.split(",");
            if (reviewsArr.length > 0) {
                for (var i in reviewsArr) {
                    if (reviewsArr[i] == "<i class=\"fa fa-toggle-on\"></i>") {
                        dialogMsg("所选数据中包含已提交的数据，请重新选择！", 0);
                        return false;
                        break;
                    }
                }
            }
            dialogConfirm("注：您确定要提交所选中的数据吗？", function (r) {
                if (r) {
                    Loading(true, "正在提交...");
                    window.setTimeout(function () {
                        $.ajax({
                            url: "../../SteelMember/RawMaterialOrder/SubmitReview",
                            data: { keyValues: keyValues },
                            type: "post",
                            dataType: "json",
                            success: function (data) {
                                if (data.type == "3") {
                                    dialogAlert(data.message, -1);
                                } else {
                                    Loading(false);
                                    dialogMsg(data.message, 1);
                                }
                                $('#gridTable').trigger('reloadGrid');
                            }
                        });
                    }, 500);
                }
            });
        }
    }

    //审核
    function btn_passed(operat, type) {
        var keyValue = $('#gridTable').jqGridRowValue('OrderId');
        var reviews = $('#gridTable').jqGridRowValue('IsSubmit');

        var ReviewMan1 = $('#gridTable').jqGridRowValue('ReviewMan1');
        var ReviewMan2 = $('#gridTable').jqGridRowValue('ReviewMan2');
        var ReviewMan3 = $('#gridTable').jqGridRowValue('ReviewMan3');
        var ReviewMan4 = $('#gridTable').jqGridRowValue('ReviewMan4');
        var ReviewMan5 = $('#gridTable').jqGridRowValue('ReviewMan5');

        if (checkedArray(keyValue)) {

            var reviewsArr = reviews.split(",");
            var ReviewMan1Arr = ReviewMan1.split(",");
            var ReviewMan2Arr = ReviewMan2.split(",");
            var ReviewMan3Arr = ReviewMan3.split(",");
            var ReviewMan4Arr = ReviewMan4.split(",");
            var ReviewMan5Arr = ReviewMan5.split(",");
            if (operat == 1) {
                if (reviewsArr.length > 0) {
                    for (var i in reviewsArr) {
                        if (reviewsArr[i] == "<i class=\"fa fa-toggle-off\"></i>") {
                            dialogMsg("所选数据中包含未提交的数据，请重新选择！", 0);
                            return false;
                        }
                    }
                }
                if (ReviewMan1Arr.length > 0) {
                    for (var i in ReviewMan1Arr) {
                        if (ReviewMan1Arr[i] == "<span class=\"label label-success\">通过</span>" || ReviewMan1Arr[i] == "<span class=\"label label-danger\">失败</span>") {
                            dialogMsg("所选数据中包含已审核的数据，请重新选择！", 0);
                            return false;
                        }
                    }
                }
            } else if (operat == 2) {
                if (ReviewMan1Arr.length > 0) {
                    for (var i in ReviewMan1Arr) {
                        if (ReviewMan1Arr[i] == "<span class=\"label label-info\">未审核</span>") {
                            dialogMsg("所选数据中包含构件厂长未审核的数据，请重新选择！", 0);
                            return false;
                        }
                    }
                }
                if (ReviewMan2Arr.length > 0) {
                    for (var i in ReviewMan2Arr) {
                        if (ReviewMan2Arr[i] == "<span class=\"label label-success\">通过</span>" || ReviewMan2Arr[i] == "<span class=\"label label-danger\">失败</span>") {
                            dialogMsg("所选数据中包含已审核的数据，请重新选择！", 0);
                            return false;
                        }
                    }
                }
            } else if (operat == 3) {
                if (ReviewMan2Arr.length > 0) {
                    for (var i in ReviewMan2Arr) {
                        if (ReviewMan2Arr[i] == "<span class=\"label label-info\">未审核</span>") {
                            dialogMsg("所选数据中包含工程部长未审核的数据，请重新选择！", 0);
                            return false;
                        }
                    }
                }
                if (ReviewMan3Arr.length > 0) {
                    for (var i in ReviewMan3Arr) {
                        if (ReviewMan3Arr[i] == "<span class=\"label label-success\">通过</span>" || ReviewMan3Arr[i] == "<span class=\"label label-danger\">失败</span>") {
                            dialogMsg("所选数据中包含已审核的数据，请重新选择！", 0);
                            return false;
                        }
                    }
                }
            } else if (operat == 4) {
                if (ReviewMan3Arr.length > 0) {
                    for (var i in ReviewMan3Arr) {
                        if (ReviewMan3Arr[i] == "<span class=\"label label-info\">未审核</span>") {
                            dialogMsg("所选数据中包含物质部长未审核的数据，请重新选择！", 0);
                            return false;
                        }
                    }
                }
                if (ReviewMan4Arr.length > 0) {
                    for (var i in ReviewMan4Arr) {
                        if (ReviewMan4Arr[i] == "<span class=\"label label-success\">通过</span>" || ReviewMan4Arr[i] == "<span class=\"label label-danger\">失败</span>") {
                            dialogMsg("所选数据中包含已审核的数据，请重新选择！", 0);
                            return false;
                        }
                    }
                }
            } else {
                if (ReviewMan4Arr.length > 0) {
                    for (var i in ReviewMan4Arr) {
                        if (ReviewMan4Arr[i] == "<span class=\"label label-info\">未审核</span>") {
                            dialogMsg("所选数据中包含副总工未审核的数据，请重新选择！", 0);
                            return false;
                        }
                    }
                }
                if (ReviewMan5Arr.length > 0) {
                    for (var i in ReviewMan5Arr) {
                        if (ReviewMan5Arr[i] == "<span class=\"label label-success\">通过</span>" || ReviewMan5Arr[i] == "<span class=\"label label-danger\">失败</span>") {
                            dialogMsg("所选数据中包含已审核的数据，请重新选择！", 0);
                            return false;
                        }
                    }
                }
            }

            var message = "";
            if (type == 1) {
                message = "注：您确定要对所选中的数据进行审核通过吗？";
            } else {
                message = "注：您确定要对所选中的数据进行审核失败吗？";
            }
            dialogConfirm(message, function (r) {
                if (r) {
                    Loading(true, "正在处理...");
                    window.setTimeout(function () {

                        if (type == 1) {
                            $.SaveForm({
                                url: "../../SteelMember/RawMaterialOrder/ReviewOperation?keyValue=" + keyValue + "&operat=" + operat + "&type=" + type,
                                param: {},
                                loading: "正在保存数据...",
                                success: function () {
                                    $.currentIframe().$("#gridTable").trigger("reloadGrid");
                                }
                            })
                        } else {
                            dialogOpen({
                                id: 'NotReviewForm',
                                title: '失败原因',
                                url: '/SteelMember/RawMaterialOrder/NotReviewForm?keyValue=' + keyValue + "&operat=" + operat + "&type=" + type,
                                width: '400px',
                                height: '300px',
                                callBack: function (iframeId) {
                                    top.frames[iframeId].AcceptClick();
                                }
                            });
                        }
                    }, 500);
                }
            });
        }
    }

    // 收货
    function btn_Received() {
        //IsPurchase
        var keyValues = $('#gridTable').jqGridRowValue('OrderId');
        var passeds = $('#gridTable').jqGridRowValue('IsPassed');
        var IsReceived = $('#gridTable').jqGridRowValue('IsReceived');
        var OrderNumbering = $('#gridTable').jqGridRowValue('OrderNumbering');
        if (checkedArray(keyValues)) {
            var reviewsArr = IsReceived.split(",");
            var passedsArr = passeds.split(",");
            var OrderNumberingArr = OrderNumbering.split(",");
            if (passedsArr.length > 0) {
                for (var i in passedsArr) {//<span class="label label-info">未审核</span>
                    if (passedsArr[i] == "<span class=\"label label-info\">未审核</span>" || passedsArr[i] == "<span class=\"label label-danger\">失败</span>") {
                        dialogMsg("所选数据中包含未通过审核的数据，请先去审核！", 0);
                        return false;
                    }
                }
            }

            if (reviewsArr.length > 0) {
                for (var i in reviewsArr) {
                    if (reviewsArr[i] == "<i class=\"fa fa-toggle-on\"></i>") {
                        dialogMsg("所选数据中包含已收货的数据，请重新选择！", 0);
                        return false;
                    }
                }
            }

            if (OrderNumberingArr.length > 0) {
                for (var i in OrderNumberingArr) {
                    var OrderNumbering = OrderNumberingArr[i];
                    //alert(OrderNumbering);
                    var Data = "";
                    $.ajax({
                        url: "../../SteelMember/RawMterialCollar/GetFormJson1",
                        data: { OrderNumbering: OrderNumbering },
                        type: "post",
                        dataType: "json",
                        success: function (data) {
                            Data = data;
                            //$('#gridTable').trigger('reloadGrid');
                        }
                    });

                    if (Data.CollarNumbering == "" || Data.CollarNumbering == null || Data.CollarNumbering == 0) {
                        dialogMsg("所选数据中包含未出库的数据，请先去出库！", 0);
                        return false;
                    } else {

                        Loading(true, "正在提交...");
                        window.setTimeout(function () {
                            $.ajax({
                                url: "../../SteelMember/RawMaterialOrder/SubmitIsReceived",
                                data: { keyValues: keyValues },
                                type: "post",
                                dataType: "json",
                                success: function (data) {
                                    if (data.type == "3") {
                                        dialogAlert(data.message, -1);
                                    } else {
                                        Loading(false);
                                        dialogMsg(data.message, 1);
                                    }
                                    $('#gridTable').trigger('reloadGrid');
                                }
                            });
                        }, 500);
                    }
                }


            }
        }
    }

    function Search() {
        dialogOpen({
            id: "AdvancedSearchForm",
            title: '高级查询',
            url: '/Utility/AdvancedSearchForm?SubProjectId=' + _Category,
            width: "670px",
            height: "180px",
            callBack: function (iframeId) {
                top.frames[iframeId].AcceptClick();
            },
            btn: null,
        });
    }
</script>

